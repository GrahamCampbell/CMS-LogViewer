{
    "name": "graham-campbell/cms-logviewer",
    "description": "CMS LogViewer Is A CMS Core Plugin That Adds A LogViewer Admin Module",
    "keywords": ["laravel", "framework", "cms logviewer", "cms", "log viewer", "LogViewer", "CMS LogViewer", "CMS-LogViewer", "Graham Campbell", "GrahamCampbell"],
    "license": "GNU Affero General Public License v3",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.0.*",
        "symfony/config": "2.3.*",
        "symfony/filesystem": "2.3.*",
        "graham-campbell/cms-core": "dev-develop"
    },
    "require-dev": {
        "orchestra/testbench": "2.0.*",
        "mockery/mockery": "0.8.*",
        "phpunit/phpunit": "3.7.*",
        "sebastian/finder-facade": "1.1.*",
        "sebastianbergmann/phpcov": "1.1.0",
        "satooshi/php-coveralls": "dev-master",
        "symfony/event-dispatcher": "2.3.*",
        "symfony/stopwatch": "2.3.*",
        "symfony/yaml": "2.3.*",
        "guzzle/guzzle": "3.8.*"
    },
    "autoload": {
        "psr-0": {
            "GrahamCampbell\\CMSLogViewer\\": "src/",
            "GrahamCampbell\\Tests\\CMSLogViewer": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
